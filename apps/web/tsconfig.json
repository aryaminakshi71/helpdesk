{
  "extends": [
    "../../packages/tsconfig/react.json"
  ],
  "compilerOptions": {
    "lib": [
      "ESNext",
      "DOM"
    ],
    "target": "ESNext",
    "module": "Preserve",
    "moduleDetection": "force",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noPropertyAccessFromIndexSignature": false,
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@helpdesk/core": [
        "../../packages/core/src/index.ts"
      ],
      "@helpdesk/core/*": [
        "../../packages/core/src/*"
      ],
      "@helpdesk/auth": [
        "../../packages/auth/src/index.ts"
      ],
      "@helpdesk/auth/*": [
        "../../packages/auth/src/*"
      ],
      "@helpdesk/api": [
        "../../apps/api/src/index.ts"
      ],
      "@helpdesk/api/*": [
        "../../apps/api/src/*"
      ],
      "@helpdesk/storage": [
        "../../packages/storage/src/db/index.ts"
      ],
      "@helpdesk/storage/*": [
        "../../packages/storage/src/*"
      ],
      "@helpdesk/env": [
        "../../packages/env/src/index.ts"
      ],
      "@helpdesk/env/*": [
        "../../packages/env/src/*"
      ],
      "@helpdesk/logger": [
        "../../packages/logger/src/index.ts"
      ],
      "@helpdesk/logger/*": [
        "../../packages/logger/src/*"
      ]
    },
    "types": [
      "vite/client",
      "@helpdesk/env/cloudflare"
    ]
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.css"
  ],
  "exclude": [
    "node_modules"
  ]
}